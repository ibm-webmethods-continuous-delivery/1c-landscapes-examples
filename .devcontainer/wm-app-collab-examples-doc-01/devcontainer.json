{
  "build": {
    "dockerfile": "Dockerfile",
    "image": "1c-landscape-examples-doc-01",
    "args": {
      "__uname": "vscode",
      "__workspace_folder": "${containerWorkspaceFolder}"
    }
  },
  "remoteEnv": {
    "CONTAINER_WORKSPACE_FOLDER": "${containerWorkspaceFolder}"
  },
  "name": "1c-landscape-examples-doc-01",
  "containerUser": "vscode",
  "runArgs": [
    "--rm",
    "--hostname",
    "1c-landscape-examples-doc-01",
    "--name",
    "1c-landscape-examples-doc-01"
  ],
  "postCreateCommand": "git config --global --add safe.directory ${CONTAINER_WORKSPACE_FOLDER}",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorTheme": "Monokai Dimmed"
      },
      "extensions": [
        "eamodio.gitlens",
        "hediet.vscode-drawio",
        "mhutchie.git-graph",
        "oderwat.indent-rainbow",
        "trunk.io",
        "yzhang.markdown-all-in-one"
      ]
    }
  }